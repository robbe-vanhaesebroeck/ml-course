    "from sklearn.linear_model import LogisticRegression\n",
    "\n",
    "linear_clf = LogisticRegression()\n",
    "\n",
    "grids = [\n",
    "    {\n",
    "        'penalty': ['l2'],\n",
    "        'C': np.arange(0.5,2, 0.1)\n",
    "    },\n",
    "    {\n",
    "        'penalty': ['l1'],\n",
    "        'C': np.arange(0.5,2,0.1),\n",
    "        'solver': ['saga'],\n",
    "    },\n",
    "    {\n",
    "        'penalty': ['elasticnet'],\n",
    "        'C': np.arange(0.5,2,0.1),\n",
    "        'solver': ['saga'],\n",
    "        'l1_ratio': np.arange(0.1,1,0.1)\n",
    "    }\n",
    "]\n",
    "\n",
    "score_model(linear_clf, grids)"
   ]
  },
  {
   "cell_type": "markdown",
